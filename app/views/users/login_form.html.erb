<h1>ログイン</h1>
<div ><%= @error_message %></div>
<%= form_with url:login_path do |f| %>
  <div><%= f.label :'メールアドレス' %><div>
  <div><%= f.text_field :email %></div>
  <div><%= f.label :'パスワード' %><div>
  <div><%= f.text_field :password %></div>
  <div><%= f.submit "ログイン" %></div>
<% end %>
<div><%= link_to "トップページ", users_path %></div>
