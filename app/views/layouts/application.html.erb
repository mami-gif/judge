<!DOCTYPE html>
<html>
  <head>
    <title>Judge</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <header>
      <% if session[:user_id] %>
          <li>
            現在ログインしているユーザーのid:
             <%= session[:user_id] %>    
          </li>
          
      <% end %>
    </header>
    <%= yield %>
  </body>
</html>

